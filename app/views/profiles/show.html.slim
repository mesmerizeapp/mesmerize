= render partial: 'header'

section.pane-lg.profile-ideas
  .container
    .tab-content
      #ideas.tab-pane.active
        = render partial: 'shared/ideas', locals: { ideas: @ideas, message: 'Ideas from this user is currently in the brewing process.' }

      #voted-ideas.tab-pane
        = render partial: 'shared/ideas', locals: { ideas: @user.voted_ideas, message: 'Nothing to see here...'}
